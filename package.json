{
  "name": "triagelock",
  "version": "1.0.0",
  "description": "Rule-based Emergency Triage & Load Management System",
  "main": "dist/server.js",
  "scripts": {
    "postinstall": "cd client && npm install",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:client\"",
    "dev:backend": "nodemon src/server.ts",
    "dev:client": "cd client && npm run dev",
    "build": "npm run build:backend && npm run build:client",
    "build:backend": "tsc",
    "build:client": "cd client && npm run build",
    "start": "npm run migrate && npm run seed && NODE_ENV=production node dist/server.js",
    "populate": "node populate-database.js",
    "test": "jest",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "install:all": "npm install && cd client && npm install"
  },
  "keywords": [
    "triage",
    "emergency",
    "healthcare"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.0.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "redis": "^4.6.11",
    "socket.io": "^4.7.2",
    "sqlite3": "^5.1.7",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/node-cron": "^3.0.11",
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
